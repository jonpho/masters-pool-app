<div id="<%= dom_id golfer %>">
  <p>
    <strong>Player:</strong>
    <%= golfer.player_id %>
  </p>

  <p>
    <strong>Firstname:</strong>
    <%= golfer.firstname %>
  </p>

  <p>
    <strong>Lastname:</strong>
    <%= golfer.lastname %>
  </p>

  <p>
    <strong>Tour:</strong>
    <%= golfer.tour %>
  </p>

  <p>
    <strong>Status:</strong>
    <%= golfer.status %>
  </p>

</div>
